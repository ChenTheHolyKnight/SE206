<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="assignment.view.StatsFrameController">
   <children>
      <TableView fx:id="_playerTable" fixedCellSize="70.0" layoutX="40.0" layoutY="220.0" prefHeight="100.0" prefWidth="540.0">
        <columns>
          <TableColumn fx:id="_nameColumn" prefWidth="178.0" resizable="false" text="Name" />
          <TableColumn fx:id="_scoreColumn" prefWidth="180.0" resizable="false" text="Score" />
            <TableColumn fx:id="_attemptsColumn" prefWidth="180.0" resizable="false" text="Attempts" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <JFXButton fx:id="_button" layoutX="221.0" layoutY="476.0" prefHeight="50.0" prefWidth="178.0" text="Go Hard Level">
         <font>
            <Font size="20.0" />
         </font>
      </JFXButton>
      <Label alignment="CENTER" layoutX="107.0" layoutY="89.0" prefHeight="74.0" prefWidth="411.0" text="Statistics">
         <font>
            <Font size="51.0" />
         </font>
      </Label>
      <JFXButton layoutX="14.0" layoutY="14.0" onAction="#handleBackButton" prefHeight="50.0" prefWidth="50.0" text="&lt;">
         <font>
            <Font size="20.0" />
         </font>
      </JFXButton>
      <Label layoutX="80.0" layoutY="14.0" prefHeight="50.0" text="Go Back To Main Menu">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <JFXButton layoutX="263.0" layoutY="380.0" onAction="#handleStayButton" prefHeight="50.0" prefWidth="86.0" text="Stay">
         <font>
            <Font size="20.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
