<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="assignment.view.UserNameSceneController">
   <children>
      <VBox alignment="CENTER" layoutX="124.0" layoutY="105.0" prefHeight="380.0" prefWidth="360.0" spacing="20.0">
         <children>
            <Label alignment="CENTER" prefHeight="46.0" prefWidth="380.0" text="Please enter a name to continue :">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField fx:id="_textField" onKeyReleased="#setButtonRealsed" prefHeight="40.0" prefWidth="492.0">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <JFXButton fx:id="_button" onAction="#handleButton" prefHeight="47.0" prefWidth="118.0" style="-fx-background-color: #c19a6b;" text="Continue">
               <font>
                  <Font size="20.0" />
               </font>
            </JFXButton>
         </children>
      </VBox>
      <Button layoutX="14.0" layoutY="21.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="40.0" prefWidth="40.0" text="&lt;">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Label layoutX="70.0" layoutY="21.0" prefHeight="40.0" text="Back to Main Menu">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <JFXButton buttonType="RAISED" layoutX="14.0" layoutY="21.0" onAction="#handleBackButton" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: #c19a6b;" text="&lt;">
         <font>
            <Font size="19.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
